{
  "private": true,
  "name": "remix-app-template-js",
  "description": "",
  "license": "",
  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -o ./app/styles/wallet.css",
    "dev": "concurrently \"npm run dev:css\" \"npm run dev:js\"",
    "dev:css": "tailwindcss -o ./app/styles/chrises.css --watch",
    "dev:js": "cross-env NODE_ENV=development netlify dev",
    "postinstall": "remix setup node",
    "preinstall": "npm run dev:css",
    "start": "remix-serve build"
  },
  "dependencies": {
    "@netlify/functions": "^0.10.0",
    "@remix-run/netlify": "^1.1.3",
    "@remix-run/react": "^1.1.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-vertical-timeline-component": "^3.5.2",
    "remix": "^1.1.3"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.1.3",
    "autoprefixer": "^10.4.2",
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "postcss": "^8.4.6",
    "tailwindcss": "^3.0.19"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
